{
  "name": "@smk17/treer",
  "version": "1.0.6",
  "description": "a directory structure tree generator",
  "bin": {
    "treer": "src/index.js"
  },
  "main": "index.js",
  "author": "Seng Mitnick",
  "license": "MIT",
  "keywords": [
    "directory",
    "dir",
    "folder",
    "structure",
    "tree"
  ],
  "dependencies": {
    "commander": "^2.9.0",
    "graphology": "^0.25.4",
    "graphology-metrics": "^2.2.0",
    "ignore": "^5.2.4",
    "ngraph.graph": "^20.0.1",
    "tiktoken": "^1.0.11",
    "tree-sitter": "^0.20.6",
    "tree-sitter-css": "^0.19.0",
    "tree-sitter-html": "^0.20.0",
    "tree-sitter-javascript": "^0.20.1",
    "tree-sitter-python": "^0.20.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sengmitnick/treer.git"
  },
  "bugs": {
    "url": "https://github.com/sengmitnick/treer/issues"
  },
  "type": "module",
  "scripts": {
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/repomap.js",
    "repomap": "node --experimental-modules --es-module-specifier-resolution=node src/test_repomap.js"
  }
